# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/boot         emmc   /dev/block/bootdevice/by-name/boot      flags=backup=1;display=Boot
/cache        auto   /dev/block/bootdevice/by-name/cache     flags=backup=1;display=Cache
/data         auto   /dev/block/bootdevice/by-name/userdata  flags=backup=1;display=Data;encryptable=footer
/system       auto   /dev/block/bootdevice/by-name/system    flags=backup=1;display=System;wipeingui
/cust         auto   /dev/block/bootdevice/by-name/cust      flags=backup=1;display=Custom
/oeminfo      emmc   /dev/block/bootdevice/by-name/oeminfo   flags=backup=1;display=OEMinfo;flashimg=1
/persist      auto   /dev/block/bootdevice/by-name/persist   flags=backup=1;display=Persist
/misc         emmc   /dev/block/bootdevice/by-name/misc	     flags=backup=1;display=Misc
/recovery     emmc   /dev/block/bootdevice/by-name/recovery  flags=backup=1;display=Recovery
/erecovery    emmc   /dev/block/bootdevice/by-name/erecovery flags=backup=1;display=eRecovery
/efs1         emmc   /dev/block/bootdevice/by-name/modemst1  flags=backup=1;display=EFS
/efs2         emmc   /dev/block/bootdevice/by-name/modemst2  flags=backup=1;subpartitionof=/efs1
/firmware     auto   /dev/block/bootdevice/by-name/modem     flags=backup=1;display=Firmware;mounttodecrypt

/external_sd  auto   /dev/block/mmcblk1p1 /dev/block/mmcblk1 flags=display="Micro SDcard";storage;wipeingui;removable;andsec
/sd-ext       auto   /dev/block/mmcblk1p2                    flags=display="Adopted storage, use with Micro SDcard and data";wipeingui;removable
/usb-otg      auto   /dev/block/sda1      /dev/block/sda     flags=removable;storage;display=USB-OTG
